{
  "name": "laren",
  "version": "1.0.0",
  "description": "Command line tool for renaming files with custom lambda expression",
  "main": "index.js",
  "editions": [
    {
      "description": "Source + ESNext + Require",
      "entry": "src/index.js"
    },
    {
      "description": "Babel Compiled + ES2015 + Require",
      "entry": "es2015/index.js"
    }
  ],
  "bin": {
    "laren": "./es2015/index.js"
  },
  "scripts": {
    "test": "ava tests/**/*.js --fail-fast",
    "build": "babel ./src --out-dir ./es2015",
    "projectz": "projectz compile",
    "compile": "npm run build && npm run projectz"
  },
  "keywords": [
    "rename",
    "cli"
  ],
  "author": "Metál Ádám",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devmetal/laren.git"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "editions": "^1.1.2",
    "glob": "^7.0.6",
    "babel-polyfill": "^6.13.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.14.0"
  },
  "maintainers": [],
  "contributors": [],
  "bugs": {
    "url": "https://github.com/devmetal/laren/issues"
  }
}
